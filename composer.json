{
    "name": "exewen/otto-php-sdk",
    "description": "A otto-php-sdk component for Exewen.",
    "authors": [
        {
            "name": "exewen",
            "email": "exeweb@163.com"
        }
    ],
    "license": "MIT",
    "type": "library",
    "keywords": [
        "php",
        "exewen",
        "otto-sdk"
    ],
    "require": {
        "php": ">=7.3",
        "exewen/facades": "^1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 | ^6.5 | ^7.5 | ^8.4 | ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Exewen\\Otto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExewenTest\\Otto\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "extra": {
        "exewen": {
            "config": "Exewen\\Otto\\ConfigRegister"
        }
    }

}
